package exo3;

import java.net.Socket;

public class ClientDaytime {
	
	
	public void run() {
		Socket sc; 
		
		try {
			sc = new Socket ("localhost", 13);
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		
	}
	
	public static void main(String[] args) {
		
		ClientDaytime cd = new ClientDaytime();
		cd.run();
		
	}



}
